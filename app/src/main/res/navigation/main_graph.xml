<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.ajou.helpt.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_setUserInfoFragment"
            app:destination="@id/setUserInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/setUserInfoFragment"
        android:name="com.ajou.helpt.SetUserInfoFragment"
        android:label="fragment_set_user_info"
        tools:layout="@layout/fragment_set_user_info" />
</navigation>